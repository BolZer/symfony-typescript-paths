{
  "name": "bolzer/symfony-typescript-routes",
  "description": "A service to generate typescript paths from the router of an symfony application",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Jan Noehles",
      "email": "noehles.jan@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bolzer\\SymfonyTypescriptRoutes\\": ""
    }
  },
  "require": {
    "php": ">=8.2",
    "symfony/config": "^v6.1",
    "symfony/dependency-injection": "^v6.1",
    "symfony/http-kernel": "^v6.1",
    "symfony/routing": "^v6.1",
    "symfony/validator": "^v6.1",
    "symfony/yaml": "^v6.1"
  }
  ,
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpspec/prophecy": "1.17.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.5.20",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "5.16.0"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    }
  }
}
